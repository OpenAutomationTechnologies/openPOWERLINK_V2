# Makefile generated by Xilinx SDK.

-include libgen.options

LIBRARIES = ${PROCESSOR}/lib/libxil.a
MSS = system.mss

all: libs
	@echo 'Finished building libraries'

libs: $(LIBRARIES)

$(LIBRARIES): $(MSS)
	libgen -hw ${HWSPEC}\
	       ${REPOSITORIES}\
	       -pe ${PROCESSOR} \
	       -log libgen.log \
	       $(MSS)

clean:
	rm -rf ${PROCESSOR}
